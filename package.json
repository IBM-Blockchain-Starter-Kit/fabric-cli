{
    "name": "fabric-cli",
    "version": "1.0.1",
    "description": "Simple CLI to normalize how to interact with fabric",
    "main": "fabric-cli.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc -w",
        "test": "LOGGING_LEVEL=error jest",
        "test:watch": "LOGGING_LEVEL=error jest --watchAll",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-ca-client": "~1.4.0",
        "fabric-client": "~1.4.0",
        "fs-extra": "^2.0.0",
        "log4js": "^0.6.38",
        "yargs": "^10.0.3"
    },
    "bin": {
        "fabric-cli": "./dist/fabric-cli.js"
    },
    "devDependencies": {
        "@types/jest": "^24.0.11",
        "jest": "^24.7.1",
        "ts-jest": "^24.0.2",
        "typescript": "^3.4.3"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "node"
        ],
        "reporters": [
            "default"
        ]
    }
}
